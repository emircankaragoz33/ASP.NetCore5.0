@using EntityLayer.Concrete
@model List<WebUser>

<section class="page-section" id="services">
    <div class="container px-4 px-lg-5">
        <h2 class="text-center mt-0">Eğitmen Ekibimiz</h2>
        <hr class="divider" />

        <div class="row gx-4 gx-lg-5">
            @foreach (var item in Model)
            {
                
           
            <div class="col-lg-3 col-md-6 text-center">
                <div class="mt-5">
                    <div class="mb-2"><img src="/memberImage/@item.KucukFotograf" width="100" height="100" /></div>
                    <h3 class="h4 mb-2">@item.Name @item.Surname</h3>
                    <p class="text-muted mb-0">@item.UzmanlikAlan</p>
                </div>
            </div>
            }
        </div>
    </div>
</section>